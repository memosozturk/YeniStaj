@model List<YeniStaj.Models.Entities.Project>

@{
    ViewBag.Title = "Export";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Export</h2>



<div class="card">
    <div class="card-header">

    </div>
    <div class="card-block">
        <!-- DataTables init on table by adding .js-dataTable-full class, functionality initialized in js/pages/base_tables_datatables.js -->
        <table id="example" class="table table-bordered table-striped table-vcenter  nowrap display dataTable">
            <thead>
                <tr>
                    <th class="text-center">#</th>

                    <th>Project Name</th>
                    <th>Description</th>

                    <th>Create Time</th>

                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    var data = Model[i];
                    <tr class="" :"bg-red")">
                        <td class="text-center">@(i + 1)</td>



                        <td>@data.ProjeAdi</td>

                        <td>@data.ProjeAciklama</td>
                        <td>@data.EklenmeTarihi</td>


                    </tr>
                }
            </tbody>
        </table>
    </div>
    <!-- .card-block -->
</div>





